---
layout: none
---
[
{% assign posts_with_title = site.pages | where_exp: "post", "post.title" %}
{% for post in posts_with_title %}
{
"title"    : "{{ post.title | escape }}",
"category" : "{{ post.category }}",
"tags"     : "{{ post.tags | join: ', ' }}",
"url"      : "{{ site.baseurl }}{{ post.url }}",
"date"     : "{{ post.date }}"
} {% unless forloop.last %},{% endunless %}
{% endfor %}
]